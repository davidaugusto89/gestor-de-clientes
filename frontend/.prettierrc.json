{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 80,
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": true,
  "bracketSameLine": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "printWidth": 80
      }
    }
  ]
}
